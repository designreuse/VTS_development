    xmlns:aop="http://www.springframework.org/schema/aop"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:security="http://www.springframework.org/schema/security"
    xmlns:tx="http://www.springframework.org/schema/tx"
    xsi:schemaLocation="
        http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
        http://www.springframework.org/schema/security http://www.springframework.org/schema/security/spring-security-3.0.xsd
        http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.0.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<bean id="urlMapping"
		class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		<property name="interceptors">
			<list>
				<ref bean="defaultLoginInterceptor" />
			</list>
		</property>
		<property name="mappings">
			<value>
				/login.* loginController
				/home.* homeController
				/passkey.* passkeyController
				/main.* mainController
				/wifi.* wifiController
				/busdetailshome.* busDeatilsController
				/farechartupload.* fareChartUploadController
				/terminaldeviceusers.* terminalDeviceUsersController
				/busStopshome.* busStopshomeController
				/conductorhome.* conductorController
				/Configfilehome.* ConfigfilehomeController
				/depothome.* depotController
				/divisionhome.* divisionController
				/driverhome.* driverController
				/routehome.* routeController
				/routeschedule.* routeScheduleController
				/typedetails.* typeDetailsController
				/healthsync.* healthSyncController
				/reportsgen.* reportsController
				/transaction.* transactionController
				/terminaldevicehome.* terminalController
				/operator.* operatorController
				/inputValidate.* inputvalidatorController
				/outputValidate.* outputValidateController
				/busInputFileValidator.* busInputFileValidatorController
				/stopsInputFileValidator.* stopsInputFileValidatorController
				/conductorInputFileValidator.* conductorInputFileValidatorController
				/depotInputFileValidator.* depotInputFileValidatorController
				/divisionInputFileValidator.* divisionInputFileValidatorController
				/driverInputFileValidator.* driverInputFileValidatorController
				/routesInputFileValidator.* routesInputFileValidatorController
				/usersInputFileValidator.* usersInputFileValidatorController
				/wifiInputFileValidator.* wifiInputFileValidatorController
				/farechartInputFileValidator.* farechartInputFileValidatorController
				/typeInputFileValidator.* typeInputFileValidatorController
				/busOutputFileValidator.* busOutputFileValidatorController
				/stopsOutputFileValidator.* stopsOutputFileValidatorController
				/conductorOutputFileValidator.* conductorOutputFileValidatorController
				/depotOutputFileValidator.* depotOutputFileValidatorController
				/divisionOutputFileValidator.* divisionOutputFileValidatorController
				/driverOutputFileValidator.* driverOutputFileValidatorController
				/routesOutputFileValidator.* routesOutputFileValidatorController
				/usersOutputFileValidator.* usersOutputFileValidatorController
				/wifiOutputFileValidator.* wifiOutputFileValidatorController
				/farechartOutputFileValidator.* farechartOutputFileValidatorController
				/typeOutputFileValidator.* typeOutputFileValidatorController
				/alertReport.* alertReportController
				/update.* reportEditableController
			</value>
		</property>
	</bean>

	<bean id="defaultLoginInterceptor" class="com.harman.its.controllers.LoginInterceptor">
		<property name="failureView" value="/HITS-UI/login.do" />
		<property name="escapeViews">
			<list>
				<value> .*/login.* </value>
				<value> .*/main.* </value>
				<value> .*/passkey.* </value>
			</list>
		</property>
	</bean>
	<bean id="viewResolver"
		class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="prefix">
			<value>/WEB-INF/views/</value>
		</property>
		<property name="suffix">
			<value>.jsp</value>
		</property>
	</bean>

	<bean id="multipartResolver"
		class="org.springframework.web.multipart.commons.CommonsMultipartResolver">
		<property name="maxUploadSize" value="50000000" />
	</bean>

